{
    "name": "park-manager/park-manager",
    "type": "project",
    "description": "Open-source hosting solution",
    "keywords": ["ftp", "mail", "server", "hosting"],
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "ParkManager\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": "^7.0",
        "doctrine/doctrine-bundle": "^1.6.4",
        "doctrine/doctrine-cache-bundle": "^1.2.2",
        "doctrine/orm": "^2.5.6",
        "incenteev/composer-parameter-handler": "^2.1.2",
        "roave/security-advisories": "dev-master",
        "sensio/distribution-bundle": "^5.0.9",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/monolog-bundle": "^2.8.2",
        "symfony/swiftmailer-bundle": "^2.4.2",
        "symfony/symfony": "~3.2.1"
    },
    "require-dev": {
        "behat/behat": "^3.2.2",
        "behat/mink": "^1.7.1",
        "behat/mink-browserkit-driver": "^1.3.2",
        "behat/mink-extension": "^2.1.0",
        "behat/symfony2-extension": "^2.1.1",
        "matthiasnoback/symfony-service-definition-validator": "^1.2.6",
        "phpunit/phpunit": "^5.7.4",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "symfony/phpunit-bridge": "^3.2.1"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
