{
    "name": "park-manager/park-manager",
    "type": "project",
    "description": "Open-source hosting solution",
    "keywords": ["ftp", "mail", "server", "hosting"],
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "beberlei/assert": "^2.7.6",
        "ramsey/uuid": "^3.6.1",
        "symfony/console": "^3.3@dev",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.3@dev",
        "symfony/framework-bundle": "^3.3@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^2.4@dev",
        "symfony/security-bundle": "^3.3@dev",
        "symfony/twig-bundle": "^3.3@dev",
        "symfony/validator": "^3.3@dev",
        "symfony/yaml": "^3.3@dev"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^2.1.0",
        "phpspec/phpspec": "^3.0@dev",
        "phpunit/phpunit": "^6.1@dev",
        "ramsey/uuid-console": "^1.1.1",
        "symfony/dotenv": "^3.3",
        "symfony/phpunit-bridge": "^3.3",
        "symfony/web-profiler-bundle": "^3.3@dev",
        "symfony/web-server-bundle": "^3.3@dev"
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "ParkManager\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %WEB_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "<3.3",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BF6RBYGY5V4S3FDMBNKXVVNS",
            "allow-contrib": false
        }
    }
}
