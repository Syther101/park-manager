{
    "name": "park-manager/service-bus-bridge",
    "type": "library",
    "description": "Park-Manager ServiceBus Bridge",
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "park-manager/service-bus": "^0.2",
        "symfony/dependency-injection": "^4.0",
        "league/tactician-container": "^2.0.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Bridge\\ServiceBus\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Bridge\\ServiceBus\\Tests\\": "Tests"
        }
    }
}
