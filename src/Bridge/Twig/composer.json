{
    "name": "park-manager/twig",
    "type": "library",
    "description": "Park-Manager Twig Bridge",
    "homepage": "http://www.park-manager.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Bridge\\Twig\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Bridge\\Twig\\Tests\\": "Tests"
        }
    }
}
