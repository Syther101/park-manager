{
    "name": "park-manager/user-bundle",
    "type": "symfony-bundle",
    "description": "Park-Manager UserBundle",
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "hostnet/form-handler-bundle": "^1.4.1",
        "park-manager/doctrine-bridge": "^0.1",
        "park-manager/user": "^0.1",
        "prooph/service-bus-symfony-bundle": "^1.0",
        "sylius/mailer": "^1.0",
        "sylius/mailer-bundle": "^1.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/security-bundle": "^3.4 || ^4.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/twig-bundle": "^3.4 || ^4.0",
        "symfony/validator": "^3.4 || ^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Bundle\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/",
            "Resources/"
        ]
    },
    "autoload-dev": {
        "ParkManager\\Bundle\\UserBundle\\Tests\\": "Tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
