{
    "name": "park-manager/route-autofill-bundle",
    "type": "symfony-bundle",
    "description": "RouteAutofillBundle can auto-fill missing missing route parameter when generating",
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Bundle\\RouteAutofillBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Bundle\\RouteAutofillBundle\\Tests\\": "Tests"
        }
    }
}
