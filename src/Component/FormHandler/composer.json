{
    "name": "park-manager/form-handler",
    "type": "library",
    "description": "Park-Manager FormHandler Component",
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "park-manager/application-foundation": "^0.3",
        "symfony/form": "^4.1",
        "symfony/validator": "^4.1"
    },
    "require-dev": {
        "symfony/http-foundation": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Component\\FormHandler\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Component\\FormHandler\\Tests\\": "Tests"
        }
    }
}
