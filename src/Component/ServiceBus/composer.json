{
    "name": "park-manager/service-bus",
    "type": "library",
    "description": "Park-Manager ServiceBus Component",
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "league/tactician": "^1.0",
        "psr/container": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "symfony/debug": "^4.0.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Component\\ServiceBus\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Component\\ServiceBus\\Tests\\": "Tests"
        }
    }
}
