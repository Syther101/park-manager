{
    "name": "park-manager/module",
    "type": "library",
    "description": "Park-Manager Module Component",
    "homepage": "http://www.park-manager.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "doctrine/dbal": "^2.7",
        "doctrine/doctrine-bundle": "^1.8",
        "rollerworks/route-autowiring-bundle": "^1.0.2",
        "symfony/finder": "^4.1",
        "symfony/phpunit-bridge": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Component\\Module\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Component\\Module\\Tests\\": "Tests"
        }
    },
    "minimum-stability": "dev"
}
